pipeline{
    agent any

    tools{
        maven "jenkins-mvn"
    }

    stages{

        stage('Git Checkout'){
            steps{

            git 'https://github.com/RamanDxc/RamanDxc.git'
            }
        }

        stage('Maven Build'){
            steps{
            bat """
                cd mywebapp
                mvn clean package
            """
            }
        }

    }
}
